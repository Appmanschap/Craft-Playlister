{
    "name": "appmanschap/craft-playlister",
    "description": "Import Playlists directly into CraftCMS",
    "version": "1.0.0",
    "type": "craft-plugin",
    "license": "proprietary",
    "support": {
        "email": "info@appmanschap.nl",
        "issues": "https://github.com/github.com:appmanschap/craft-playlister.git/issues?state=open",
        "source": "https://github.com/github.com:appmanschap/craft-playlister.git",
        "docs": "https://github.com/github.com:appmanschap/craft-playlister.git",
        "rss": "https://github.com/github.com:appmanschap/craft-playlister.git/releases.atom"
    },
    "authors": [
        {
            "name": "appmanschap",
            "homepage": "https://www.appmanschap.nl/"
        }
    ],
    "require": {
        "php": ">=8.2",
        "craftcms/cms": "^5.0.0",
        "google/apiclient": "^2.15.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "appmanschap\\craftplaylister\\": "src/"
        }
    },
    "extra": {
        "handle": "craft-playlister",
        "name": "Playlister",
        "developer": "Appmanschap",
        "documentationUrl": "https://github.com/github.com:appmanschap/youtube-playlist-importer.git",
        "class": "appmanschap\\craftplaylister\\Playlister"
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --ansi --fix",
        "phpstan": "vendor/bin/phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
